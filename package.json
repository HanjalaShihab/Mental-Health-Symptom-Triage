{
  "name": "mental-health-symptom-triage",
  "version": "1.0.0",
  "description": "A comprehensive mental health symptom triage application",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "dev": "node server/index.js",
    "start": "node server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\""
  },
  "keywords": [
    "mental-health",
    "symptom-triage",
    "assessment",
    "healthcare"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
